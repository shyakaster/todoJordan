<%if obj.errors.any? %>
  <h4>Please correct the errors below</h4>
  <%obj.errors.full_messages.each do |msg|%>
    <ul>
	    <li>
	    	<%=msg%>
	    </li>
    </ul>
  <%end%>  
<%end%>

<%=form_for obj do |f|%>
  <p>
  	<%=f.label :description%>
    <%=f.text_field :description%>
  </p>
  <p>
  	<%=f.submit "Submit"%>
  </p>  
    
<%end%>
<%= link_to "Back", listings_path%>